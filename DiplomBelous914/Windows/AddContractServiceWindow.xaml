<Window x:Class="DiplomBelous914.Windows.AddContractServiceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiplomBelous914.Windows"
        mc:Ignorable="d"
        Title="AddContractServiceWindow" Height="600" Width="800"
          WindowStartupLocation="CenterScreen">

    <Grid>


        <StackPanel Grid.Column="2" >

            <TextBlock
            HorizontalAlignment="Center"
            FontSize="20" Text="Договор"/>
            <TextBox
            x:Name="TbContract"
            IsReadOnly="True"
            HorizontalAlignment="Center"
            FontSize="20"
            Width="200"
            Margin="0 10 0 0" 
            Background="#FFF1B189"/>
            <TextBlock
     HorizontalAlignment="Center"
     FontSize="20" Text="Услуга"/>
            <ComboBox Width="194" x:Name="cmbService" />
            <TextBlock
                       HorizontalAlignment="Center"
                       FontSize="20"
                       Text="Дата"
                       Margin="0 10 0 0 "/>
            <DatePicker x:Name="StartdatePicker" Width="194"/>
            <TextBlock
                HorizontalAlignment="Center"
                FontSize="20" Text="Работник"/>
            <ComboBox Width="194" x:Name="cmbFIOWorker" />
            <!--<TextBlock
                      HorizontalAlignment="Center"
                      FontSize="20"
                      Text="Работник"
                      Margin="0 10 0 0 "/>
            <ComboBox Width="194" 
                      x:Name="cmbWorker" 
                      ItemTemplate="{StaticResource ClientTemplate}"/>-->
