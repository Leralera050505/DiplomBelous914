     HorizontalAlignment="Center"
     FontSize="20"
     Text="Комментарий"
     Margin="0 10 0 0 "/>
            <TextBox
     x:Name="TbComment"
     HorizontalAlignment="Center"
     FontSize="20"
     Width="200"
     Margin="0 10 0 0" 
     Background="#FFF1B189"/>
            <Button
     x:Name="btnSave"
     Content="Сохранить"
     FontSize="20"
     Width="200"
     Margin="0 10 0 0" Click="btnSave_Click"
     Background="#FF9E73" 
     Style="{StaticResource  ButtonStileBa}" Height="35">
                <Button.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </Button.Resources>
            </Button>
            <Button
           x:Name="btnClose"
           Content="Отмена"
           FontSize="20"
           Width="200"
           Margin="0 10 0 0" Click="btnClose_Click" 
           Background="#FF9E73" 
           Style="{StaticResource  ButtonStileBa}">

                <Button.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </Button.Resources>
            </Button>
        </StackPanel>

        <Image x:Name="___Res_1_png" Margin="-173,-38,0,0"
