     HorizontalAlignment="Center"
     FontSize="20"
 
     Text="Фамилия работника"
     Margin="0 10 0 0 "/>
            <TextBox
     x:Name="TbFirstNameW"
     HorizontalAlignment="Center"
     FontSize="20"
     Width="200"
     Margin="0 10 0 0" 
     Background="#FFF1B189"/>

            <TextBlock
     HorizontalAlignment="Center"
     FontSize="20"
 
     Text="Имя работника"
     Margin="0 10 0 0 "/>
            <TextBox

     HorizontalAlignment="Center"
     FontSize="20"
     Width="200"
     x:Name="TbLastNameW"
     Margin="0 10 0 0" 
     Background="#FFF1B189"/>

            <TextBlock
     HorizontalAlignment="Center"
     FontSize="20"
             
     Text="Отчество  работника"
     Margin="0 10 0 0 "/>
            <TextBox

     HorizontalAlignment="Center"
     FontSize="20"
     Width="200"
     x:Name="TbPatronymicW"
